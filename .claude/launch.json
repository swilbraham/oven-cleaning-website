{
  "version": "0.0.1",
  "configurations": [
    {
      "name": "oven-cleaning-site",
      "runtimeExecutable": "node",
      "runtimeArgs": ["-e", "const http=require('http'),fs=require('fs'),path=require('path'),mime={'.html':'text/html','.css':'text/css','.js':'application/javascript','.png':'image/png','.jpg':'image/jpeg','.svg':'image/svg+xml','.ico':'image/x-icon'};http.createServer((q,r)=>{let u=q.url.split('?')[0];if(u==='/')u='/index.html';const f=path.join('D:/Websites/Oven Cleaning',u),e=path.extname(f);fs.readFile(f,(err,d)=>{if(err){r.writeHead(404);r.end('Not found');return;}r.writeHead(200,{'Content-Type':mime[e]||'application/octet-stream'});r.end(d);});}).listen(3000,()=>console.log('Server running on port 3000'));"],
      "port": 3000
    }
  ]
}
